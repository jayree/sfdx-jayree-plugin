{
  "name": "sfdx-jayree",
  "description": "sfdx ALM commands & more",
  "version": "4.0.2",
  "author": "jayree <jayree.git@icloud.com>",
  "bugs": "https://github.com/jayree/sfdx-jayree-plugin/issues",
  "dependencies": {
    "@jayree/sfdx-plugin-prettier": "^1.1.0",
    "@oclif/config": "^1.17.0",
    "@salesforce/command": "^4.0.4",
    "@salesforce/core": "^2.27.2",
    "@salesforce/kit": "^1.5.17",
    "@salesforce/source-deploy-retrieve": "^4.1.1",
    "adm-zip": "^0.5.5",
    "chalk": "^4.1.2",
    "cli-ux": "^5.6.3",
    "debug": "^4.3.2",
    "enquirer": "^2.3.6",
    "execa": "^5.1.1",
    "fast-deep-equal": "^3.1.3",
    "fast-xml-parser": "^3.19.0",
    "fs-extra": "^10.0.0",
    "globby": "^11.0.4",
    "inquirer": "^8.1.2",
    "is-docker": "^2.2.1",
    "listr2": "^3.11.0",
    "marked": "^3.0.0",
    "marked-terminal": "^4.1.1",
    "object-path": "^0.11.5",
    "open": "^8.2.1",
    "puppeteer": "^10.2.0",
    "tabletojson": "^2.0.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/plugin-help": "^3.2.3",
    "@salesforce/dev-config": "^2.1.2",
    "@salesforce/dev-scripts": "0.9.18",
    "@salesforce/prettier-config": "^0.0.2",
    "@salesforce/ts-sinon": "^1.3.21",
    "@types/adm-zip": "^0.4.34",
    "@types/chai": "^4.2.21",
    "@types/fs-extra": "^9.0.12",
    "@types/inquirer": "^7.3.3",
    "@types/jsforce": "^1.9.34",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.7.1",
    "@types/sinon": "^10.0.2",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "chai": "^4.3.4",
    "codecov": "^3.8.3",
    "deepmerge": "^4.2.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-salesforce": "^0.1.6",
    "eslint-config-salesforce-typescript": "^0.2.7",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-prettier": "^3.4.1",
    "husky": "^7.0.1",
    "is-ci": "^3.0.0",
    "mocha": "^9.1.0",
    "nock": "^13.1.2",
    "nyc": "^15.1.0",
    "oclif": "^1.18.1",
    "patch-package": "^6.4.7",
    "pinst": "^2.1.6",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "sinon": "^11.1.2",
    "source-map-support": "^0.5.19",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/lib/src",
    "/lib/config",
    "/lib/metadata",
    "/messages",
    "/oclif.manifest.json",
    "/CHANGELOG.md",
    "/yarn.lock"
  ],
  "homepage": "https://github.com/jayree/sfdx-jayree-plugin",
  "keywords": [
    "ALM",
    "country",
    "state",
    "picklist",
    "git",
    "diff",
    "deployment",
    "full",
    "partial",
    "automation",
    "change set",
    "manifest",
    "package xml",
    "package description",
    "configure",
    "streaming",
    "open",
    "retrieve",
    "fix",
    "hook",
    "flow test coverage",
    "lightning sync status",
    "sfdx",
    "sfdx-plugin"
  ],
  "license": "BSD-3-Clause",
  "oclif": {
    "commands": "./lib/src/commands",
    "bin": "sfdx",
    "hooks": {
      "prerun": "./lib/src/hooks/prerun",
      "update": "./lib/src/hooks/changelog",
      "preretrieve": "./lib/src/hooks/preretrieve",
      "postretrieve": "./lib/src/hooks/postretrieve",
      "postsourceupdate": "./lib/src/hooks/postsourceupdate"
    },
    "topics": {
      "jayree": {
        "name": "jayree",
        "description": "packages and automation toolset",
        "subtopics": {
          "packagedescription": {
            "description": "add/modify/remove the description of a package"
          },
          "org": {
            "description": "manage your orgs"
          },
          "manifest": {
            "description": "generate and manipulate manifest files"
          },
          "manifest:git": {
            "description": "generate manifest files based on git data"
          },
          "source": {
            "description": "sync your project with your orgs"
          },
          "source:tracking": {
            "description": "manipulate local source tracking files"
          },
          "source:tracking:store": {
            "description": "get and set/store revision counter number"
          },
          "automation": {
            "description": "headless browser automation tools"
          },
          "automation:changeset": {
            "description": "deploy/list incomming change sets (beta)"
          },
          "automation:ltngsync": {
            "description": "check the Lightning Sync User Sync Status and reset sync if needed (beta)"
          },
          "automation:country": {
            "description": "import (create/update) Country Picklist"
          },
          "automation:state": {
            "description": "import (create/update) State/Country Picklists"
          }
        }
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "plugins": [
      "@jayree/sfdx-plugin-prettier"
    ]
  },
  "repository": "jayree/sfdx-jayree-plugin",
  "scripts": {
    "build": "sf-build",
    "clean": "sf-clean",
    "clean-all": "sf-clean all",
    "compile": "sf-compile",
    "format": "sf-format",
    "lint": "sf-lint",
    "lint-fix": "sf-lint --fix",
    "postcompile": "oclif-dev manifest && oclif-dev readme",
    "postinstall": "patch-package",
    "postpack": "rimraf oclif.manifest.json",
    "postpublish": "pinst --enable",
    "posttest": "git checkout -q -- test/assets/*",
    "prepack": "sf-prepack",
    "prepare": "is-ci || husky install",
    "prepublishOnly": "pinst --disable",
    "pretest": "sf-compile-test",
    "test": "sf-test",
    "version": "oclif-dev manifest && oclif-dev readme"
  },
  "release": {
    "branches": [
      "main",
      "next"
    ]
  }
}
