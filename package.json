{
  "name": "sfdx-jayree",
  "version": "0.0.5",
  "bugs": "https://github.com/jayree/sfdx-jayree/issues",
  "dependencies": {
    "@oclif/command": "1.5.0",
    "@oclif/config": "1.7.2",
    "@oclif/errors": "1.2.0",
    "@salesforce/command": "0.2.3",
    "@types/jsonfile": "4.0.1",
    "adm-zip": "0.4.11",
    "async": "2.6.1",
    "js2xmlparser": "3.0.0",
    "jsonfile": "4.0.0",
    "node-notifier": "5.2.1",
    "snyk": "^1.93.0",
    "tslib": "1",
    "x2js": "3.2.1",
    "xml-js": "1.6.7"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1",
    "@oclif/plugin-help": "2",
    "@oclif/test": "^1",
    "@salesforce/dev-config": "1.1.4",
    "@types/adm-zip": "0.4.31",
    "@types/chai": "4",
    "@types/jsforce": "1.8.16",
    "@types/mocha": "5",
    "@types/node": "10",
    "chai": "^4",
    "codecov": "3.0.4",
    "globby": "8",
    "mocha": "^5",
    "nyc": "13.0.1",
    "sinon": "6",
    "ts-node": "7",
    "typescript": "3.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/jayree/sfdx-jayree",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "topics": {
      "jayree:packagedescription": {
        "description": "Tools to add/modify/remove the description in a package"
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "jayree/sfdx-jayree",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json npm-shrinkwrap.json",
    "posttest": "tsc -p test --noEmit && tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme && npm shrinkwrap",
    "prepare": "npm run snyk-protect; rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme && sed -i '' '/* /d' ./README.md && npm shrinkwrap",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "snyk-protect": "snyk protect"
  },
  "snyk": true
}
