{
  "name": "sfdx-jayree",
  "description": "some sfdx commands",
  "version": "0.4.0",
  "author": "jayree @jayree",
  "bugs": "https://github.com/jayree/sfdx-jayree-plugin/issues",
  "dependencies": {
    "@oclif/command": "1.5.8",
    "@oclif/config": "1.10.2",
    "@oclif/errors": "1.2.2",
    "@salesforce/command": "1.1.1",
    "@salesforce/core": "1.1.0",
    "adm-zip": "0.4.11",
    "inquirer": "6.2.1",
    "opn": "5.4.0",
    "puppeteer": "1.11.0",
    "serialize-error": "3.0.0",
    "tslib": "1.9.3",
    "xml-js": "1.6.8"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.21.0",
    "@oclif/plugin-help": "2.1.4",
    "@oclif/semantic-release": "3.1.1",
    "@oclif/test": "1.2.2",
    "@oclif/tslint": "3.1.1",
    "@salesforce/dev-scripts": "0.3.9",
    "@types/adm-zip": "0.4.31",
    "@types/inquirer": "0.0.43",
    "@types/jsforce": "1.9.2",
    "@types/puppeteer": "1.11.1",
    "codecov": "4.0.0-0",
    "commitlint": "7.2.1",
    "globby": "8.0.1",
    "husky": "1.3.0",
    "path-exists-cli": "1.0.0",
    "semantic-release": "15.13.1",
    "tslint-config-prettier": "1.17.0",
    "typescript-tslint-plugin": "0.2.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/oclif.manifest.json",
    "/patches",
    "/yarn.lock"
  ],
  "homepage": "https://github.com/jayree/sfdx-jayree-plugin",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "topics": {
      "jayree": {
        "name": "jayree",
        "description": "packages and automation toolset",
        "subtopics": {
          "packagedescription": {
            "name": "packagedescription",
            "description": "add/modify/remove the description of a package"
          },
          "org": {
            "name": "org",
            "description": "manage your orgs"
          },
          "automation": {
            "name": "automation",
            "description": "headless browser automation tools",
            "subtopics": {
              "changeset": {
                "name": "changeset",
                "description": "deploy/list incomming change sets"
              },
              "ltngsyncstatus": {
                "name": "ltngsyncstatus",
                "description": "check the Lightning Sync User Sync Status and reset sync if needed"
              }
            }
          }
        }
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "bin": "sfdx-jayree"
  },
  "repository": "jayree/sfdx-jayree-plugin",
  "scripts": {
    "build": "yarn sfdx-build && oclif-dev manifest && oclif-dev readme",
    "clean": "yarn sfdx-clean",
    "clean-all": "yarn sfdx-clean all",
    "compile": "yarn sfdx-compile",
    "docs": "yarn sfdx-docs",
    "format": "yarn sfdx-format",
    "lint": "yarn sfdx-lint",
    "lint-fix": "yarn sfdx-lint --fix",
    "postcompile": "tsc -p test",
    "postpack": "rimraf oclif.manifest.json",
    "prepack": "yarn sfdx-build",
    "prepare": "path-exists src && yarn sfdx-build || path-exists lib",
    "test": "yarn sfdx-test",
    "posttest": "git checkout -q -- test/assets/*",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "version": "oclif-dev manifest && oclif-dev readme && git add README.md"
  },
  "husky": {
    "hooks": {
      "commit-msg": "yarn sfdx-husky-commit-msg",
      "pre-commit": "yarn sfdx-husky-pre-commit",
      "pre-push": "yarn sfdx-husky-pre-push"
    }
  }
}
