{
  "name": "sfdx-jayree-plugin",
  "version": "0.0.5",
  "author": "jayree @jayree",
  "bugs": "https://github.com/jayree/sfdx-jayree-plugin/issues",
  "dependencies": {
    "@oclif/command": "1",
    "@oclif/config": "1",
    "@oclif/errors": "1",
    "@salesforce/command": "0.2.5",
    "adm-zip": "0.4.11",
    "node-notifier": "5.2.1",
    "snyk": "1.96.0",
    "tslib": "1",
    "xml-js": "1.6.7"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/test": "^1",
    "@salesforce/dev-config": "1.1.4",
    "@types/adm-zip": "0.4.31",
    "@types/chai": "^4",
    "@types/jsforce": "1.9.0",
    "@types/jsonfile": "4.0.1",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/node-notifier": "0.0.28",
    "chai": "^4",
    "codecov": "3.1.0",
    "globby": "^8",
    "greenkeeper-lockfile": "1.15.1",
    "mocha": "^5",
    "nyc": "^13",
    "sinon": "5",
    "ts-node": "^7",
    "typescript": "^3.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/jayree/sfdx-jayree-plugin",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "topics": {
      "jayree": {
        "name": "jayree",
        "description": "Tools to generate and modify packages",
        "subtopics": {
          "packagedescription": {
            "name": "packagedescription",
            "description": "Tools to add/modify/remove the description in a package"
          }
        }
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "jayree/sfdx-jayree-plugin",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json npm-shrinkwrap.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "oclif-dev manifest && oclif-dev readme && npm shrinkwrap",
    "prepare": "npm run snyk-protect && oclif-dev manifest && oclif-dev readme && npm shrinkwrap",
    "snyk-protect": "snyk protect",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "snyk": true
}
