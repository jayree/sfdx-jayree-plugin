{
  "name": "sfdx-jayree",
  "description": "sfdx ALM commands & more",
  "version": "3.6.0",
  "author": "jayree @jayree",
  "bugs": "https://github.com/jayree/sfdx-jayree-plugin/issues",
  "dependencies": {
    "@oclif/config": "^1.17.0",
    "@salesforce/command": "^3.1.0",
    "@salesforce/core": "^2.20.5",
    "@salesforce/kit": "^1.4.5",
    "adm-zip": "^0.5.4",
    "chalk": "^4.1.0",
    "cli-ux": "^5.5.1",
    "debug": "^4.3.1",
    "enquirer": "^2.3.6",
    "execa": "^5.0.0",
    "fast-deep-equal": "^3.1.3",
    "fs-extra": "^9.1.0",
    "globby": "^11.0.2",
    "inquirer": "^8.0.0",
    "listr2": "^3.4.3",
    "marked": "^2.0.1",
    "marked-terminal": "^4.1.1",
    "object-path": "^0.11.5",
    "open": "^8.0.2",
    "puppeteer": "8.0.0",
    "slash": "^3.0.0",
    "tabletojson": "^2.0.4",
    "time-span": "^4.0.0",
    "xml-js": "^1.6.11",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/plugin-help": "^3.2.2",
    "@oclif/semantic-release": "^3.1.5",
    "@salesforce/dev-config": "^2.1.0",
    "@salesforce/dev-scripts": "^0.9.1",
    "@salesforce/prettier-config": "^0.0.2",
    "@salesforce/ts-sinon": "^1.3.5",
    "@types/adm-zip": "^0.4.33",
    "@types/chai": "^4.2.15",
    "@types/fs-extra": "^9.0.8",
    "@types/inquirer": "^7.3.1",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.35",
    "@types/sinon": "^9.0.11",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "chai": "^4.3.4",
    "codecov": "^3.8.1",
    "cross-spawn": "^7.0.3",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-salesforce": "^0.1.0",
    "eslint-config-salesforce-license": "^0.1.0",
    "eslint-config-salesforce-typescript": "^0.2.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^32.3.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.1.3",
    "is-ci": "^3.0.0",
    "mocha": "^8.3.2",
    "nock": "^13.0.11",
    "nyc": "^15.1.0",
    "patch-package": "^6.4.7",
    "pinst": "^2.1.6",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "semantic-release": "^17.4.2",
    "sinon": "^9.2.4",
    "source-map-support": "~0.5.19",
    "ts-node": "^9.1.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3",
    "xunit-file": "^1.0.0"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/lib/src",
    "/lib/config",
    "/lib/metadata",
    "/messages",
    "/oclif.manifest.json",
    "/manifest"
  ],
  "homepage": "https://github.com/jayree/sfdx-jayree-plugin",
  "keywords": [
    "ALM",
    "country",
    "state",
    "picklist",
    "git",
    "diff",
    "deployment",
    "full",
    "partial",
    "automation",
    "change set",
    "manifest",
    "package xml",
    "package description",
    "configure",
    "streaming",
    "open",
    "retrieve",
    "fix",
    "hook",
    "flow test coverage",
    "lightning sync status",
    "sfdx",
    "sfdx-plugin"
  ],
  "license": "BSD-3-Clause",
  "oclif": {
    "commands": "./lib/src/commands",
    "bin": "sfdx",
    "hooks": {
      "update": "./lib/src/hooks/changelog",
      "preretrieve": "./lib/src/hooks/preretrieve",
      "postretrieve": "./lib/src/hooks/postretrieve",
      "postsourceupdate": "./lib/src/hooks/postsourceupdate"
    },
    "topics": {
      "jayree": {
        "name": "jayree",
        "description": "packages and automation toolset",
        "subtopics": {
          "packagedescription": {
            "description": "add/modify/remove the description of a package"
          },
          "org": {
            "description": "manage your orgs"
          },
          "scratchorg": {
            "description": "scratch org tools"
          },
          "manifest": {
            "description": "generate and manipulate manifest files"
          },
          "manifest:git": {
            "description": "generate manifest files based on git data"
          },
          "source": {
            "description": "sync your project with your orgs"
          },
          "source:retrieve": {
            "description": "retrieve source from an org"
          },
          "automation": {
            "description": "headless browser automation tools"
          },
          "automation:changeset": {
            "description": "deploy/list incomming change sets (beta)"
          },
          "automation:ltngsync": {
            "description": "check the Lightning Sync User Sync Status and reset sync if needed (beta)"
          },
          "automation:statecountry": {
            "description": "import (create/update) State/Country Picklists"
          }
        }
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "jayree/sfdx-jayree-plugin",
  "scripts": {
    "build": "sf-build",
    "clean": "sf-clean",
    "clean-all": "sf-clean all",
    "compile": "sf-compile",
    "format": "sf-format",
    "lint": "sf-lint",
    "lint-fix": "sf-lint --fix",
    "postcompile": "oclif-dev manifest && oclif-dev readme",
    "postinstall": "patch-package && (is-ci || husky install)",
    "postpack": "rimraf oclif.manifest.json",
    "postpublish": "pinst --enable",
    "posttest": "git checkout -q -- test/assets/*",
    "prepack": "sf-prepack",
    "prepublishOnly": "pinst --disable",
    "pretest": "sf-compile-test",
    "test": "sf-test",
    "version": "oclif-dev manifest && oclif-dev readme"
  },
  "release": {
    "branches": [
      "main",
      "next"
    ]
  }
}
