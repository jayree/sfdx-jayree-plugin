{
  "name": "sfdx-jayree",
  "description": "A Salesforce CLI plugin containing commands and hooks for Salesforce Application Lifecycle Management tasks.",
  "version": "4.7.0",
  "author": "jayree <jayree.git@icloud.com>",
  "bugs": "https://github.com/jayree/sfdx-jayree-plugin/issues",
  "type": "module",
  "dependencies": {
    "@jayree/changelog": "^1.1.0",
    "@jayree/sfdx-plugin-legacy": "^1.1.36",
    "@jayree/sfdx-plugin-manifest": "^3.0.13",
    "@jayree/sfdx-plugin-org": "^1.0.2",
    "@jayree/sfdx-plugin-prettier": "^1.2.37",
    "@jayree/sfdx-plugin-source": "^1.1.1",
    "@oclif/core": "^2.8.2",
    "@salesforce/kit": "^1.9.2",
    "@salesforce/sf-plugins-core": "^2.2.11",
    "@salesforce/source-deploy-retrieve": "^8.0.4",
    "debug": "^4.3.4",
    "fast-xml-parser": "^4.2.0",
    "fs-extra": "^11.1.1",
    "ignore": "^5.2.4",
    "mkdirp": "^3.0.1",
    "object-path": "^0.11.8",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@oclif/plugin-command-snapshot": "^3.3.13",
    "@oclif/plugin-help": "^5.2.9",
    "@salesforce/dev-config": "^3.1.0",
    "@salesforce/dev-scripts": "^4.3.0",
    "@salesforce/prettier-config": "^0.0.2",
    "@salesforce/ts-sinon": "^1.4.6",
    "@types/fs-extra": "^11.0.1",
    "@types/jsforce": "^1.11.0",
    "@types/mkdirp": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.58.0",
    "c8": "^7.12.0",
    "chai": "^4.3.7",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-salesforce": "^1.2.0",
    "eslint-config-salesforce-license": "^0.2.0",
    "eslint-config-salesforce-typescript": "^1.1.1",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^43.0.7",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sf-plugin": "^1.15.1",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "mocha": "^10.2.0",
    "nock": "^13.3.1",
    "nyc": "^15.1.0",
    "oclif": "^3.8.1",
    "patch-package": "^6.5.1",
    "pinst": "^3.0.0",
    "prettier": "^2.8.7",
    "pretty-quick": "^3.1.3",
    "shx": "^0.3.4",
    "sinon": "^15.0.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "wireit": "^0.9.5"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/lib",
    "/config",
    "/messages",
    "/oclif.manifest.json",
    "/CHANGELOG.md"
  ],
  "homepage": "https://github.com/jayree/sfdx-jayree-plugin",
  "keywords": [
    "ALM",
    "country",
    "state",
    "picklist",
    "git",
    "diff",
    "deployment",
    "full",
    "partial",
    "automation",
    "change set",
    "manifest",
    "package xml",
    "package description",
    "configure",
    "streaming",
    "open",
    "retrieve",
    "fix",
    "hook",
    "flow test coverage",
    "lightning sync status",
    "sfdx",
    "sfdx-plugin"
  ],
  "license": "BSD-3-Clause",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "hooks": {
      "update": "./lib/hooks/changelog",
      "preretrieve": "./lib/hooks/preretrieve",
      "postretrieve": "./lib/hooks/postretrieve"
    },
    "topics": {
      "jayree": {
        "name": "jayree",
        "description": "manifest, source, and org automation toolset",
        "subtopics": {
          "org": {
            "description": "manage your orgs"
          },
          "source": {
            "description": "sync your project with your orgs"
          }
        }
      }
    },
    "devPlugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-command-snapshot"
    ],
    "plugins": [
      "@jayree/sfdx-plugin-prettier",
      "@jayree/sfdx-plugin-manifest",
      "@jayree/sfdx-plugin-source",
      "@jayree/sfdx-plugin-org",
      "@jayree/sfdx-plugin-legacy"
    ],
    "info": {
      "releasenotes": {
        "distTagUrl": "https://registry.npmjs.org/-/package/sfdx-jayree/dist-tags",
        "releaseNotesPath": "https://github.com/jayree/sfdx-jayree-plugin/tree/main",
        "releaseNotesFilename": "CHANGELOG.md"
      }
    }
  },
  "repository": "jayree/sfdx-jayree-plugin",
  "scripts": {
    "build": "wireit",
    "clean": "sf-clean",
    "clean-all": "sf-clean all",
    "compile": "wireit",
    "format": "wireit",
    "lint": "wireit",
    "prepack": "sf-prepack",
    "prepare": "patch-package && sf-install",
    "test": "wireit",
    "version": "oclif readme --no-aliases"
  },
  "release": {
    "branches": [
      "main",
      "next"
    ]
  },
  "wireit": {
    "build": {
      "dependencies": [
        "compile",
        "lint"
      ]
    },
    "compile": {
      "command": "tsc --build --pretty",
      "files": [
        "src/**/*.ts",
        "**/tsconfig.json",
        "messages/**"
      ],
      "output": [
        "lib/**",
        "*.tsbuildinfo"
      ],
      "clean": "if-file-deleted"
    },
    "format": {
      "command": "prettier --write \"+(src|test|schemas)/**/*.+(ts|js|json)|command-snapshot.json\"",
      "files": [
        "src/**/*.ts",
        "test/**/*.ts",
        "schemas/**/*.json",
        "command-snapshot.json",
        ".prettier*"
      ],
      "output": []
    },
    "lint": {
      "command": "eslint --color --cache --cache-location .eslintcache .",
      "files": [
        "src/**/*.ts",
        "test/**/*.ts",
        "messages/**",
        "**/.eslint*",
        "**/tsconfig.json"
      ],
      "output": []
    },
    "test": {
      "dependencies": [
        "test:deprecation-policy",
        "lint",
        "test:json-schema"
      ]
    },
    "test:deprecation-policy": {
      "command": "\"./bin/dev.js\" snapshot:compare",
      "files": [
        "src/**/*.ts"
      ],
      "output": [],
      "dependencies": [
        "compile"
      ]
    },
    "test:json-schema": {
      "command": "\"./bin/dev.js\" schema:compare",
      "files": [
        "src/**/*.ts",
        "schemas"
      ],
      "output": []
    }
  }
}
