{
  "name": "sfdx-jayree",
  "description": "A Salesforce CLI plugin containing commands and hooks for Salesforce Application Lifecycle Management tasks.",
  "version": "4.4.1",
  "author": "jayree <jayree.git@icloud.com>",
  "bugs": "https://github.com/jayree/sfdx-jayree-plugin/issues",
  "type": "module",
  "dependencies": {
    "@jayree/sfdx-plugin-legacy": "^1.1.1",
    "@jayree/sfdx-plugin-manifest": "^2.3.1",
    "@jayree/sfdx-plugin-prettier": "^1.2.1",
    "@oclif/core": "^1.16.3",
    "@salesforce/command": "^5.2.8",
    "@salesforce/kit": "^1.6.1",
    "@salesforce/source-deploy-retrieve": "^6.8.1",
    "@salesforce/source-tracking": "^2.2.3",
    "debug": "^4.3.4",
    "enquirer": "^2.3.6",
    "fast-xml-parser": "^4.0.10",
    "fs-extra": "^10.1.0",
    "ignore": "^5.2.0",
    "is-docker": "^3.0.0",
    "is-wsl": "^2.2.0",
    "listr2": "^5.0.5",
    "marked": "^4.1.1",
    "marked-terminal": "^5.1.1",
    "mkdirp": "^1.0.4",
    "object-path": "^0.11.8",
    "puppeteer": "^18.0.4",
    "semver": "^7.3.7",
    "tabletojson": "^2.0.7",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@oclif/plugin-command-snapshot": "^3",
    "@oclif/plugin-help": "^5.1.12",
    "@salesforce/dev-config": "^3.1.0",
    "@salesforce/dev-scripts": "3.1.0",
    "@salesforce/prettier-config": "^0.0.2",
    "@salesforce/ts-sinon": "^1.4.0",
    "@types/chai": "^4.3.3",
    "@types/fs-extra": "^9.0.13",
    "@types/jsforce": "^1.9.43",
    "@types/marked": "^4.0.7",
    "@types/marked-terminal": "^3.1.3",
    "@types/mkdirp": "^1.0.2",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.7.18",
    "@types/semver": "^7.3.12",
    "@types/sinon": "^10.0.13",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "chai": "^4.3.6",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-salesforce": "^1.1.0",
    "eslint-config-salesforce-typescript": "^1.1.1",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sf-plugin": "^1.1.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "mocha": "^10.0.0",
    "nock": "^13.2.9",
    "nyc": "^15.1.0",
    "oclif": "^3.2.2",
    "patch-package": "^6.4.7",
    "pinst": "^3.0.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "shx": "^0.3.4",
    "sinon": "^14.0.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/lib",
    "/config",
    "/messages",
    "/oclif.manifest.json",
    "/CHANGELOG.md"
  ],
  "homepage": "https://github.com/jayree/sfdx-jayree-plugin",
  "keywords": [
    "ALM",
    "country",
    "state",
    "picklist",
    "git",
    "diff",
    "deployment",
    "full",
    "partial",
    "automation",
    "change set",
    "manifest",
    "package xml",
    "package description",
    "configure",
    "streaming",
    "open",
    "retrieve",
    "fix",
    "hook",
    "flow test coverage",
    "lightning sync status",
    "sfdx",
    "sfdx-plugin"
  ],
  "license": "BSD-3-Clause",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "hooks": {
      "prerun": "./lib/hooks/prerun",
      "update": "./lib/hooks/changelog",
      "preretrieve": "./lib/hooks/preretrieve",
      "postretrieve": "./lib/hooks/postretrieve"
    },
    "topics": {
      "jayree": {
        "name": "jayree",
        "description": "manifest, source, and org automation toolset",
        "subtopics": {
          "org": {
            "description": "manage your orgs",
            "subtopics": {
              "configure": {
                "description": "manage state and country picklist values"
              }
            }
          },
          "source": {
            "description": "sync your project with your orgs",
            "subtopics": {
              "tracking": {
                "description": "manipulate local source tracking files",
                "subtopics": {
                  "store": {
                    "description": "get and set/store revision counter number"
                  }
                }
              }
            }
          }
        }
      }
    },
    "devPlugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-command-snapshot"
    ],
    "plugins": [
      "@jayree/sfdx-plugin-prettier",
      "@jayree/sfdx-plugin-manifest",
      "@jayree/sfdx-plugin-legacy"
    ],
    "info": {
      "releasenotes": {
        "distTagUrl": "https://registry.npmjs.org/-/package/sfdx-jayree/dist-tags",
        "releaseNotesPath": "https://github.com/jayree/sfdx-jayree-plugin/tree/main",
        "releaseNotesFilename": "CHANGELOG.md"
      }
    }
  },
  "repository": "jayree/sfdx-jayree-plugin",
  "scripts": {
    "build": "sf-build",
    "clean": "sf-clean",
    "clean-all": "sf-clean all",
    "clean:lib": "shx rm -rf lib && shx rm -rf coverage && shx rm -rf .nyc_output && shx rm -f oclif.manifest.json",
    "compile": "sf-compile",
    "format": "sf-format",
    "lint": "sf-lint",
    "lint-fix": "sf-lint --fix",
    "postcompile": "oclif readme",
    "postinstall": "patch-package",
    "postpack": "shx rm -f oclif.manifest.json",
    "postpublish": "pinst --enable",
    "posttest": "yarn lint && yarn test:deprecation-policy",
    "prepack": "sf-prepack",
    "prepare": "is-ci || husky install",
    "prepublishOnly": "pinst --disable",
    "pretest": "sf-compile-test",
    "test": "sf-test",
    "test:deprecation-policy": "./bin/dev.js snapshot:compare",
    "version": "oclif readme"
  },
  "release": {
    "branches": [
      "main",
      "next"
    ]
  }
}
